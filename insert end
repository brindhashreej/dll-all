import java.util.*;
class linked{
    Node head;
    Node tail;
    class Node{
        int data;
        Node next;
        Node prev;
        Node(int val){
            data = val;
            next = null;
            prev=null;
        }
    }
    linked(){
        head = null;
        tail=null;
    }
    public void insertend(int val){
        Node newnode = new Node(val);
        if (head==null){
            head = newnode;
            tail=newnode;
        }
        else{
            tail.next=newnode;
            newnode.prev=tail;
            tail=newnode;
        }
    }
    public void display(){
        Node temp = head;
        while(temp != null){
            System.out.print(temp.data+"-->");
            temp = temp.next;
        }
        System.out.print("Null");
    }
    public static void main(String args[]){
        Scanner sc = new Scanner (System.in);
        linked list = new linked();
        int m = sc.nextInt();
        for (int i =0 ;i<m;i++) {
            int val = sc.nextInt();
            list.insertend(val);
        }
        list.display();
    }
}
